CREATE TABLE ACCOUNTSERVICES.ACCOUNT_STATUS_AUDIT
(
  MSG_AUDIT_ID NUMBER NOT NULL ENABLE,
  CREATION_DATE TIMESTAMP(6),
  OPERATION VARCHAR2(40 CHAR),
  MESSAGE CLOB,
  CONSTRAINT PK_MSG_AUDIT_ID PRIMARY KEY(MSG_AUDIT_ID)
  
  PARTITION BY RANGE (CREATION_DATE) INTERVAL(NUMTOYMINTERVAL(1, 'MONTH'))
  (
    PARTITION ORDERHDR_PART_JAN VALUES 
        LESS THAN (TO_DATE('01-01-2019'), 'DD-MM-YY')
  )
);